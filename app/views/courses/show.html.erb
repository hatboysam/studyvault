<div class="page-header">
  <h1>
  	<%= @course.course_name %> - <%= @course.school.name %>
  	<small>
  		All Files
  	</small>
  </h1>
</div>

<% @years.each do |year| %>
	<h3><%= year %></h3>
	<%= render "uploads/uploads", 
			:uploads => @uploads.find_all{|u| u.year.year == year},
			:downloads => false %>
<% end %>
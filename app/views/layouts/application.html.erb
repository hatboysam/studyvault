	<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/jquery-ui.js"></script>
  	<%= csrf_meta_tag %>
	<%= yield :head %>
</head>
<body>
	<script>
	$(document).ready(function () {
		$(".downloadlink").click(
			function() {
				location.reload();
			});
	});	
	</script>
	
	<%= render 'shared/header' %>

	<div class = "yield round down100" id = "yield">
		<%= yield %>
	</div>


</body>
</html>
